{
  "name": "three-js-start",
  "version": "0.9.0",
  "description": "Starting with Three.js 3D javascript library.",
  "main": "app.js",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "postinstall": "webpack -p",
    "start": "node server.js",
    "lint": "eslint src",
    "test": "mocha --require src/__test/setup.js --compilers js:babel-core/register src/**/__test/*.spec.js",
    "test-watch": "nodemon --exec \"npm test\"",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover _mocha -- --bail --recursive --require src/__test/setup.js --compilers js:babel-core/register src/**/*.spec.js",
    "tdd": "nodemon --exec \"npm run coverage\""
  },
  "author": "Juan Zuriaga",
  "license": "MIT",
  "dependencies": {
    "dat.gui": "^0.6.1",
    "stats.js": "^0.17.0",
    "three": "^0.83.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "mocha-jsdom": "^1.1.0",
    "rewire": "^2.5.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
